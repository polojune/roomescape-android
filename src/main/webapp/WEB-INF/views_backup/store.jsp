				<script>
					$('#owl_carousel_store').owlCarousel({
						items: 5,
						nav: true,
						dots: true,
						dotsEach: true, /* dots 항상 표시 */

						margin: 30, /* 슬라이드 각 요소의 margin값 넣기 */

						center: true, /* 첫번째 요소가 가운데로 배치 */
						loop: true,

						navText: ['<img class="section_slider_nav_btn" src="https://www.roomescape.co.kr/_template/assets/img/theme/detail/def_info/sub_banner_slider_prev.jpg" alt="" />', '<img class="section_slider_nav_btn" src="https://www.roomescape.co.kr/_template/assets/img/theme/detail/def_info/sub_banner_slider_next.jpg" alt="" />'],

						autoplay: true,

						mouseDrag: true,
						responsiveClass: true,
						responsive: {
							0: {
								items: 1,
								loop: true
							},
							480: {
								items: 1,
								loop: true
							},
							769: {
								items: 1,
								loop: true
							},
							1000: {
								items: 3,
								loop: true
							},
							1280: {
								items: 3,
								loop: true
							},
							1400: {
								items: 3,
								loop: true
							},
							2560: {
								items: 4,
								loop: true
							}
						},
						onInitialized: function (e) {

							var idx = e.item.index;

							console.log($('#owl_carousel_store .owl-item.active').length);

							var $item_length = $('#owl_carousel_store .owl-item.active').length

							if ($item_length == 5 || $item_length == 6) {

								$('#owl_carousel_store .owl-item.middle').removeClass('middle');
								$('#owl_carousel_store .owl-item.medium').removeClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx + 2).addClass('middle');
								$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');

							} else if ($item_length == 3) {

								$('#owl_carousel_store .owl-item.middle').removeClass('middle');
								$('#owl_carousel_store .owl-item.medium').removeClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('middle');
								$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');

							} else if ($item_length == 1) {

								$('#owl_carousel_store .owl-item.middle').removeClass('middle');
								$('#owl_carousel_store .owl-item.medium').removeClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx).addClass('middle');
								$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
								$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');

							}


						}
					});

					$('#owl_carousel_store').on('initialized.owl.carousel translate.owl.carousel', function (e) {
						var idx = e.item.index;

						console.log($('#owl_carousel_store .owl-item.active').length);

						var $item_length = $('#owl_carousel_store .owl-item.active').length

						if ($item_length == 5 || $item_length == 6) {

							$('#owl_carousel_store .owl-item.middle').removeClass('middle');
							$('#owl_carousel_store .owl-item.medium').removeClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx + 2).addClass('middle');
							$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');

						} else if ($item_length == 3) {

							$('#owl_carousel_store .owl-item.middle').removeClass('middle');
							$('#owl_carousel_store .owl-item.medium').removeClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('middle');
							$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');

						} else if ($item_length == 1) {

							$('#owl_carousel_store .owl-item.middle').removeClass('middle');
							$('#owl_carousel_store .owl-item.medium').removeClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx).addClass('middle');
							$('#owl_carousel_store .owl-item').eq(idx - 1).addClass('medium');
							$('#owl_carousel_store .owl-item').eq(idx + 1).addClass('medium');
						}
					});
				</script>
